{
  "name": "kanji.sh",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/globals.css -o app/styles/app.css",
    "predev": "npm run clean",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/globals.css -o app/styles/app.css",
    "start": "remix-serve build",
    "clean": "rimraf .cache build"
  },
  "dependencies": {
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "adm-zip": "^0.5.9",
    "build-url-ts": "^6.1.3",
    "clsx": "^1.1.1",
    "formik": "^2.2.9",
    "lottie-web": "^5.9.4",
    "node-fetch": "2.6.7",
    "p-queue": "6.6.2",
    "pdf-merger-js": "^3.4.0",
    "puppeteer": "^14.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-pdf": "^5.7.2",
    "react-router-dom": "^6.3.0",
    "tiny-invariant": "^1.2.0",
    "ts-md5": "^1.2.11",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/adm-zip": "^0.5.0",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-pdf": "^5.7.2",
    "@types/yargs": "^17.0.10",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.15.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  }
}
